{
	"name": "kitty-utils-dev",
	"globIgnorePaths": ["**/*.toml", "**/luau_packages", "**/lune_packages"],
	"tree":	{
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"$path" : "src"
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"Client": {
					"$path": "dev/client.init.luau"
				}
			}
		},
		"ServerScriptService": {
			"Server": {
				"$path": "dev/server.init.luau"
			}
		}
	}
}
